    "react-hubspot-form": "^1.3.6",
    "react-redux": "^9.2.0",
    "react-scrollspy": "^3.4.3",
    "react-slick": "^0.29.0",
    "redux": "^5.0.1",
    "rework": "^1.0.1",
    "rework-pure-grids": "^2.0.1",
    "sass": "^1.63.2",
    "sass-mq": "^6.0.0",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.3",
    "three": "^0.152.2",
    "yet-another-react-lightbox": "^3.23.2"
  },
  "scripts": {
    "rework-css": "node ./src/rework-pure-grids.js",
    "gatsby-build": "gatsby build",
    "build": "npm run rework-css && npm run generate-embeddings && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "gatsby-develop": "gatsby develop",
    "develop": "npm run rework-css && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "upload": "node upload.js",
    "redirect": "node redirect.js",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "generate-embeddings": "node scripts/generate-embeddings.js",
    "generate-embeddings:force": "node scripts/generate-embeddings.js --force",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3"
  },
  "engines": {
    "node": ">=18.18.*"
  }
}
